#!/bin/bash --login
# job script that launch run.sh.

#SBATCH --time=10:00            # limit of wall clock time - how long the job will run (same as -t)

#SBATCH --ntasks=8                 # number of tasks - how many tasks (nodes) that you require (same as -n)

#SBATCH --cpus-per-task=1           # number of CPUs (or cores) per task (same as -c)

#SBATCH --mem=2GB            # memory required per allocated CPU (or core) - amount of memory (in bytes)


./run.sh



